{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "source": "/(.*)",
      "destination": "/dist"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist"
    }
  ]
}
